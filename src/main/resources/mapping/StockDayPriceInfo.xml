<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.ts.module.crawler.dao.StockDayPriceInfoDao">
	<!-- 查询记录数 --> 
    <select id="queryListCount" resultType="Int">
        SELECT count(*) FROM pc.stockdaypriceinfo
    </select>
   
    <!-- 查询记录 --> 
    <select id="queryList" resultType="java.util.HashMap" parameterType="java.util.HashMap">
        SELECT * FROM pc.stockdaypriceinfo ORDER BY fDate DESC
    	<where>
	        <if test="MD5 != null">
	         	AND MD5 = #{MD5} 
	        </if>
        </where>
        <if test="firstResult!=null and maxResult!=null">
            limit #{firstResult},#{maxResult}
        </if>
    </select>
    
    <!-- 删除 -->  
	<delete id="delete" parameterType="java.util.Map">  
	        DELETE FROM pc.stockdaypriceinfo WHERE md5 = #{md5}  
	</delete>
    
</mapper>